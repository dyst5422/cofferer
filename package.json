{
  "name": "cofferer",
  "version": "1.0.0",
  "description": "A JS/TS benchmark runner with a familiar spec-like api",
  "main": "index.js",
  "scripts": {
    "test": "npx jest",
    "build-index": "esbuild ./src/cli.ts ./src/reporter.ts --format=esm --platform=node --outdir=dist --out-extension:.js=.mjs",
    "build-worker": "esbuild ./src/worker.ts src/intendant/* src/cofferer-environment-node.ts --format=cjs --platform=node --outdir=dist",
    "build": "npm run build-index & npm run build-worker",
    "watch": "npm run build-index -- --watch",
    "watch-worker": "npm run build-worker -- --watch",
    "go": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyst5422/cofferer.git"
  },
  "keywords": [
    "benchmark",
    "runner"
  ],
  "author": "Dylan Stewart",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dyst5422/cofferer/issues"
  },
  "homepage": "https://github.com/dyst5422/cofferer#readme",
  "devDependencies": {
    "@types/co": "^4.6.2",
    "@types/dedent": "^0.7.0",
    "@types/stack-utils": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "esbuild": "^0.12.16",
    "eslint": "^7.31.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "co": "^4.6.0",
    "commander": "^8.2.0",
    "cosmiconfig": "^7.0.0",
    "dedent": "^0.7.0",
    "immer": "^9.0.5",
    "is-generator-fn": "^2.1.0",
    "jest-each": "^27.2.4",
    "jest-environment-node": "^27.2.4",
    "jest-haste-map": "^27.2.4",
    "jest-util": "^27.2.4",
    "jest-worker": "^27.2.4",
    "jest-resolve": "^27.2.4",
    "jest-runtime": "^27.2.4",
    "pretty-format": "^27.2.4",
    "ts-invariant": "^0.9.3"
  }
}
